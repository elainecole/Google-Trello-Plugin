{
  "manifest_version": 2,

  "name": "Robin",
  "description": "This extension integrates Trello into Google Calendar",
  "version": "1.0",

  "browser_action": {
    "default_icon": "robin.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
